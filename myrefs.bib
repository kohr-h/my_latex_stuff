	keywords = {electron microscopy, stem imaging, tomography},
	pages = {70--80},
	file = {Aoyama et al. - 2008 - STEM tomography for thick biological specimens.pdf:/home/hkohr/zotero/storage/A7WX897J/Aoyama et al. - 2008 - STEM tomography for thick biological specimens.pdf:application/pdf}
}

