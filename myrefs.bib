	keywords = {electron microscopy, modeling},
	pages = {19--32},
	file = {Vulovi{\'c} et al. - 2013 - Image formation modeling in cryo-electron microsco.pdf:/home/hkohr/zotero/storage/EH4PD8DB/Vulovi{\'c} et al. - 2013 - Image formation modeling in cryo-electron microsco.pdf:application/pdf}
