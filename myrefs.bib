	pages = {39--51},
	file = {Unser et al. - 1987 - A new resolution criterion based on spectral signa.pdf:/home/hkohr/zotero/storage/3M4KG7Z4/Unser et al. - 1987 - A new resolution criterion based on spectral signa.pdf:application/pdf}
}

@article{grimm_electron_1998,
	title = {Electron {Tomography} of {Ice}-{Embedded} {Prokaryotic} {Cells}},
